<%- include('partials/header') %> 
<body>
    <%- include('partials/navbar') %> 
    <div class="container">
        <h1>This Random - <%= num %> </h1>
        <!-- if-else statement -->
        <% if(num % 2 === 0){%> 
            <h2>This is an Even Number</h2>
        <% } else { %>
            <h2>This is an Odd Number</h2> 
        <% } %> 

        <h3><%= num % 2 === 0 ? 'EVEN' : 'ODD' %></h3>
    </div>
</body>
</html>